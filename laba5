#include <iostream>
#include <stdlib.h>
#include <stdio.h>
#define N 5

struct List
{
    int inf;
    int next;
};

void init(struct List *head)
{
    head[0].next = 0;
    for (int i = 1; i < N; i++)
    {
        head[i].next = -1;
    }
}

int main()
{
    struct List array[N];
    init(array);
    return (0);
}
